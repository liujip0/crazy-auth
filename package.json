{
  "name": "@crazy-auth/monorepo",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --names app,api -c green,blue \"pnpm app:dev\" \"pnpm api:dev\"",
    "app:dev": "cd app && pnpm dev",
    "api:dev": "cd api && pnpm dev",
    "create-environment": "cp app/.env.local.example app/.env.local && cp api/.dev.vars.example api/.dev.vars",
    "create-environment-windows": "cd app && copy .env.local.example .env.local && cd ../api && copy .dev.vars.example .dev.vars",
    "up-deps": "pnpm update && cd app && pnpm update && cd ../api && pnpm update"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "prettier": "^3.6.2"
  }
}
